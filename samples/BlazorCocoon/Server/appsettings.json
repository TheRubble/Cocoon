{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Cocoon": {
    "Proxy": {
      "DestinationPrefix": "http://legacyapp:8080/"
    },
    "Authentication": {
      "BackendApiUrl": "http://legacyapp:8080/facadeauth",
      "LoginUrl": "/Account/Login?ReturnUrl={{ReturnUrl}}",
      "Cookies": [
        ".AspNet.ApplicationCookie"
      ]
    },
    "Session": {
      "BackendApiUrl": "http://legacyapp:8080/facadesession",
      "Cookies": [
        "ASP.NET_SessionId"
      ]
    },
    "Cookies": {
      "BackendApiUrl": "http://legacyapp:8080/facadecookies"
    }
  }
}
